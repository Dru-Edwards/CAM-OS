{
  "name": "cam-arbitration-mesh-cdk",
  "version": "1.0.0",
  "description": "AWS CDK deployment for Complete Arbitration Mesh",
  "bin": {
    "cam-arbitration-mesh-cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:staging": "cdk deploy --all --context environment=staging", 
    "deploy:prod": "cdk deploy --all --context environment=prod",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.87.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.87.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "arbitration",
    "mesh",
    "infrastructure",
    "ecs",
    "fargate"
  ],
  "author": "CAM Team",
  "license": "MIT"
}
