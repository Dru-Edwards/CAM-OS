{
  "name": "cam-performance-testing",
  "version": "2.0.0",
  "description": "Performance testing framework for Complete Arbitration Mesh",
  "scripts": {
    "test:load:basic": "./scripts/run-load-test.sh --scenario=basic",
    "test:load:heavy": "./scripts/run-load-test.sh --scenario=heavy",
    "test:load:peak": "./scripts/run-load-test.sh --scenario=peak",
    "test:load:spike": "./scripts/run-load-test.sh --scenario=spike",
    "test:load:all": "./scripts/run-load-test.sh --scenario=all",
    "test:stress:memory": "./scripts/run-stress-test.sh --type=memory",
    "test:stress:concurrency": "./scripts/run-stress-test.sh --type=concurrency",
    "test:stress:network": "./scripts/run-stress-test.sh --type=network",
    "test:stress:all": "./scripts/run-stress-test.sh --type=all",
    "test:benchmark:arbitration": "./scripts/run-benchmark.sh --category=arbitration",
    "test:benchmark:agents": "./scripts/run-benchmark.sh --category=agent-collaboration",
    "test:benchmark:providers": "./scripts/run-benchmark.sh --category=provider-comparison",
    "test:benchmark:cost": "./scripts/run-benchmark.sh --category=cost-optimization",
    "test:benchmark:full": "./scripts/run-benchmark.sh --category=all",
    "test:benchmark:quick": "./scripts/run-benchmark.sh --category=quick",
    "test:endurance": "./scripts/run-endurance-test.sh",
    "test:regression-check": "./scripts/regression-check.sh",
    "test:scalability": "./scripts/run-scalability-test.sh",
    "analyze:results": "node utils/data-analyzer.js",
    "generate:report": "./scripts/generate-report.sh",
    "profile:memory": "clinic doctor -- node --max-old-space-size=4096 src/app.js",
    "profile:cpu": "clinic flame -- node src/app.js",
    "profile:heap": "clinic heapprofiler -- node src/app.js",
    "setup:environment": "./scripts/setup-test-environment.sh",
    "cleanup:results": "rm -rf results/*",
    "validate:config": "node utils/validate-config.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "artillery": "^2.0.3",
    "clinic": "^12.1.0",
    "k6": "^0.47.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/complete-arbitration-mesh-final.git",
    "directory": "tests/performance"
  },
  "keywords": [
    "performance-testing",
    "load-testing",
    "benchmarking",
    "arbitration-mesh",
    "ai-optimization"
  ],
  "author": "CAM Development Team",
  "license": "MIT"
}
