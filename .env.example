# CAM Protocol Environment Configuration Example

# Node environment
NODE_ENV=development

# Server configuration
PORT=8080
HOST=localhost
API_BASE_URL=http://localhost:8080

# Authentication
JWT_SECRET=your-jwt-secret-key-change-in-production
JWT_EXPIRY=24h

# Logging
LOG_LEVEL=info

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_test_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Stripe Product IDs
STRIPE_COMMUNITY_PRODUCT_ID=prod_community
STRIPE_PROFESSIONAL_PRODUCT_ID=prod_professional
STRIPE_ENTERPRISE_PRODUCT_ID=prod_enterprise

# Stripe Price IDs
STRIPE_COMMUNITY_PRICE_ID=price_community
STRIPE_PROFESSIONAL_PRICE_ID=price_professional
STRIPE_ENTERPRISE_PRICE_ID=price_enterprise

# Checkout Configuration
CHECKOUT_SUCCESS_URL=http://localhost:8080/checkout/success
CHECKOUT_CANCEL_URL=http://localhost:8080/checkout/cancel

# AI Provider Keys (for routing)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_AI_API_KEY=your-google-ai-api-key

# Database Configuration (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/cam_protocol
