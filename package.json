{
  "name": "@cam-protocol/complete-arbitration-mesh",
  "version": "2.0.0",
  "description": "Intelligent orchestration and collaboration for modern AI ecosystems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "author": "CAM Protocol Team",
  "homepage": "https://github.com/cam-protocol/complete-arbitration-mesh",
  "repository": {
    "type": "git",
    "url": "https://github.com/cam-protocol/complete-arbitration-mesh.git"
  },
  "bugs": {
    "url": "https://github.com/cam-protocol/complete-arbitration-mesh/issues"
  },
  "keywords": [
    "ai",
    "arbitration",
    "routing",
    "collaboration",
    "agents",
    "machine-learning",
    "orchestration",
    "iacp"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "tsc",
    "build:full": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "generate:badges": "vitest run --coverage && node scripts/generate-coverage-badges.js",
    "test:performance": "ts-node scripts/run-benchmarks.ts",
    "benchmark:cost": "ts-node tests/benchmarks/cost-optimization-benchmark.ts",
    "benchmark:collaboration": "ts-node tests/benchmarks/multi-agent-collaboration-benchmark.ts",
    "demo:value": "ts-node examples/demonstration/value-demonstration.ts",
    "run:benchmarks": "ts-node scripts/run-benchmarks.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,json,md}",
    "clean": "rimraf dist",
    "docker:build": "docker build -t cam-protocol/complete-arbitration-mesh .",
    "docker:run": "docker run -p 8080:8080 cam-protocol/complete-arbitration-mesh",
    "deploy:staging": "npm run build && npm run deploy:staging:only",
    "deploy:production": "npm run build && npm run deploy:production:only"
  },  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "fastify": "^4.28.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.75.1",
    "ora": "^5.4.1",
    "pino": "^8.21.0",
    "pino-pretty": "^11.2.2",
    "stripe": "^18.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^16.18.104",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^0.34.6",
    "chart.js": "^4.4.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^4.5.5",
    "vitest": "^0.34.6"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE-CORE",
    "LICENSE-ENTERPRISE"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
