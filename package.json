{
  "name": "@cam-protocol/complete-arbitration-mesh",
  "version": "2.0.0",
  "description": "Intelligent orchestration and collaboration for modern AI ecosystems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "author": "CAM Protocol Team",
  "homepage": "https://github.com/cam-protocol/complete-arbitration-mesh",
  "repository": {
    "type": "git",
    "url": "https://github.com/cam-protocol/complete-arbitration-mesh.git"
  },
  "bugs": {
    "url": "https://github.com/cam-protocol/complete-arbitration-mesh/issues"
  },
  "keywords": [
    "ai",
    "arbitration",
    "routing",
    "collaboration",
    "agents",
    "machine-learning",
    "orchestration",
    "iacp"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "generate:badges": "jest-coverage-badges --output ./badges",
    "test:performance": "ts-node scripts/run-benchmarks.ts",
    "benchmark:cost": "ts-node tests/benchmarks/cost-optimization-benchmark.ts",
    "benchmark:collaboration": "ts-node tests/benchmarks/multi-agent-collaboration-benchmark.ts",
    "demo:value": "ts-node examples/demonstration/value-demonstration.ts",
    "run:benchmarks": "ts-node scripts/run-benchmarks.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,json,md}",
    "clean": "rimraf dist",
    "docker:build": "docker build -t cam-protocol/complete-arbitration-mesh .",
    "docker:run": "docker run -p 8080:8080 cam-protocol/complete-arbitration-mesh",
    "deploy:staging": "npm run build && npm run deploy:staging:only",
    "deploy:production": "npm run build && npm run deploy:production:only"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "fastify": "^4.26.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.35.0",
    "ora": "^8.0.1",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "stripe": "^18.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/cli-table3": "^0.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-v8": "^1.4.0",
    "chart.js": "^4.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vite": "^5.2.6",
    "vitest": "^1.4.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE-CORE",
    "LICENSE-ENTERPRISE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
